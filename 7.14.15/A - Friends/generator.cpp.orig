#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <stack>
#include <queue>
#include <vector>
#include <ctime>

int main() {
  srand(time(NULL));
  printf("100\n");
  for (int i = 0; i < 100; i++) {
    printf("5 8\n");
    for (int j = 0; j < 8; j++) {
      int a = 1, b = 1;
      while (a == b) {
        a = rand() % 5 + 1;
        b = rand() % 5 + 1;
      }
      printf("%d %d\n", a, b);
    }
  }

  return 0;
}
